<article class="file-view">
  <%= hidden_field_tag "item[photo_ids][]", @item.id, class: "photo-id" %>
  <div class="thumb">
    <%= image_tag @item.image.thumb_url, width: 120, eight: 90 %>
  </div>
  <div class="name">
    <%= @item.name %>
  </div>
  <div class="action delete">
    <%= link_to t("views.button.delete"), "#", :onclick => "deleteContent('#{@item.id}'); return false;" %>
  </div>
</article>
