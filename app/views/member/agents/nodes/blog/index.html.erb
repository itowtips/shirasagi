<div class="member-blogs blogs">
  <% @items.each do |item| %>
    <article class="blog thumb">
      <%= image_tag item.thumb_url, class: :thumb %>
      <header><h2><%= link_to item.name, item.url %></h2></header>
      <div class="description">
        <%= item.description %>
      </div>
    </article>
  <% end %>
</div>

<%= paginate @items if @items.try(:current_page) %>
