<dl class="see">
  <dt><%= @model.t :name %><%= @model.tt :name %></dt>
  <dd><%= f.text_field :name %></dd>

  <dt><%= @model.t :email %><%= @model.tt :email %></dt>
  <dd><%= f.email_field :email %></dd>

  <dt><%= @model.t :in_password %><%= @model.tt :in_password %></dt>
  <dd><%= f.password_field :in_password %></dd>

  <dt><%= @model.t :state %><%= @model.tt :state %></dt>
  <dd><%= f.select :state, @item.state_options  %></dd>

  <dt><%= @model.t :oauth_type %><%= @model.tt :oauth_type %></dt>
  <dd><%= f.text_field :oauth_type %></dd>

  <dt><%= @model.t :oauth_id %><%= @model.tt :oauth_id %></dt>
  <dd><%= f.text_field :oauth_id %></dd>

  <dt><%= @model.t :oauth_token %><%= @model.tt :oauth_token %></dt>
  <dd><%= f.text_field :oauth_token %></dd>
</dl>
