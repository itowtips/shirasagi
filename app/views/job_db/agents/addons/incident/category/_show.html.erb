<dl class="see mod-job_db-incident-category">
  <dt><%= @model.t :category_ids %></dt>
  <dd>
    <% @item.categories.each do |category| %>
      <% next unless category %>
      <span class="job_db-category-label" style="<%= category_label_css(category) %>"><%= category.trailing_name %></span>
    <% end %>
  </dd>
</dl>
