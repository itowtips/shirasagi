<%= node_navi :circle do %>

<% if params[:action] =~ /show|edit/ %>
  <h3><%= link_to :"circle.image", circle_images_path(cid: @item.id), class: "icon-page" %></h3>
  <h3><%= link_to :"circle.map", circle_maps_path(cid: @item.id), class: "icon-page" %></h3>
<% else %>
  <h3><%= link_to :"circle.page", circle_pages_path, class: "icon-node" %></h3>
<% end %>

<% end %>
